{
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "outDir": "dist",
    "declaration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "esModuleInterop": true
  }
}
